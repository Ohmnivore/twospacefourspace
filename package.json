{
    "name": "twospacefourspace",
    "displayName": "twospacefourspace",
    "description": "This is a four-space and two-space indentation interconverter.",
    "version": "0.2.0",
    "publisher": "Ohmnivore",
    "license": "Do what you want",
    "categories": [
        "Other"
    ],
    "keywords": [
        "two",
        "four",
        "space",
        "indentation",
        "indent"
    ],
    "preview": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Ohmnivore/twospacefourspace.git"
    },
    "bugs": {
        "url": "https://github.com/Ohmnivore/twospacefourspace.git"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "activationEvents": [
        "onCommand:twospacefourspace.twoToFour",
        "onCommand:twospacefourspace.fourToTwo"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "twospacefourspace.twoToFour",
                "title": "Two to four (indentation)"
            },
            {
                "command": "twospacefourspace.fourToTwo",
                "title": "Four to two (indentation)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}